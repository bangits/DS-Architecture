    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.2",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "jsdom": "^22.1.0",
    "semantic-release": "^21.0.9",
    "tsc-alias": "^1.8.7",
    "tsconfig": "*",
    "typescript": "^4.9.5",
    "vitest": "^0.34.2"
  },
  "dependencies": {
    "@my-ui/core": "*",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "@my-ui/core": "*",
    "react": "^18.2.0"
